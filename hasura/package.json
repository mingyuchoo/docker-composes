{
  "name": "hasura",
  "version": "1.0.0",
  "description": "You can run docker containers for hasura",
  "main": "docker-compose.yaml",
  "scripts": {
    "docker:build": "docker-compose --file docker-compose.yaml up --build --detach",
    "docker:exec": "docker exec -it hasura_engine /bin/bash",
    "docker:exec:root": "docker exec -u root -it hasura_engine /bin/bash"
  },
  "keywords": [
    "docker",
    "hasura"
  ],
