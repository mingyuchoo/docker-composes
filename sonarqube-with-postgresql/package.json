{
  "name": "sonarqube-with-postgresql",
  "version": "1.0.0",
  "description": "You can run docker containers for pact broker",
  "main": "docker-compose.yml",
  "scripts": {
    "docker:build": "docker-compose --file docker-compose.yml up --build --detach",
    "docker:exec:sonarqube": "docker exec --interactive=true --tty=true sonarqube /bin/bash",
    "docker:exec:sonarqube-db": "docker exec --interactive=true --tty=true sonarqube-db /bin/bash"
  },
  "keywords": [
    "pact",
    "sonarqube",
    "docker",
    "postgresql"
  ],
  "author": "mingyuchoo@gmail.com",
  "license": "MIT License"
}
