{
  "name": "hasura-with-postgresql",
  "version": "1.0.0",
  "description": "You can run docker containers for hasura",
  "main": "docker-compose.yaml",
  "scripts": {
    "console": "npm run -s hasura:console:dev",
    "hasura:init": "hasura init .",
    "hasura:console:dev": "hasura console",
    "docker:build": "docker-compose --file docker-compose.yaml up --build --detach",
    "docker:exec:hasura": "docker exec -it hasura_engine /bin/bash",
    "docker:exec:hasura:root": "docker exec -u root -it hasura_engine /bin/bash",
    "docker:exec:db": "docker exec -it hasura_db /bin/bash"
  },
  "keywords": [
    "docker",
    "postgresql",
    "hasura"
  ],
  "author": "mingyuchoo@gmail.com",
  "license": "MIT License",
  "devDependencies": {
    "hasura-cli": "^2.0.9"
  }
}
