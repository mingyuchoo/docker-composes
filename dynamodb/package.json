{
  "name": "dynamo",
  "version": "1.0.0",
  "description": "You can run docker containers for dynamodb",
  "main": "docker-compose.yml",
  "scripts": {
    "docker:up": "docker-compose --file docker-compose.yml up --build --detach",
    "docker:down": "docker-compose --file docker-compose.yml down --volumes --remove-orphans",
    "docker:exec": "docker exec -it dynamodb-local /bin/bash",
    "docker:exec:root": "docker exec -u root -it dynamodb-local /bin/bash"
  },
  "keywords": [
    "docker",
    "dynamo"
  ],
  "author": "mingyuchoo@gmail.com",
  "license": "MIT License"
}
